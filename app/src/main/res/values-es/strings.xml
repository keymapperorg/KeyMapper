    <string name="title_pref_long_press_delay">Default long press delay (ms)</string>
    <string name="summary_pref_long_press_delay">How long a button should be pressed for it to be detected as a long press. Default is 500ms. Can be overridden in a keymap\'s options.</string>
    <string name="title_pref_double_press_delay">Default double press duration (ms)</string>
    <string name="summary_pref_double_press_delay">How fast does a button have to be double pressed for it to be detected as a double press. Default is 300ms. Can be overridden in a keymap\'s options.</string>
    <string name="summary_pref_vibrate_duration">How long to vibrate if vibrating is enabled for a remap. Default is 200ms. Can be overridden in a keymap\'s options.</string>
    <string name="title_pref_vibrate_duration">Default vibrate duration (ms)</string>
    <string name="summary_pref_hold_down_delay">How long a key map needs to be held down for the action to start repeat. Default is 400ms. Can be overridden in a keymap\'s options.</string>
    <string name="title_pref_hold_down_delay">Default hold down delay (ms)</string>
    <string name="summary_pref_repeat_delay">The delay between every time an action is repeated. Default is 50ms. Can be overridden in a keymap\'s options.</string>
    <string name="title_pref_repeat_delay">Default repeat delay (ms)</string>
    <string name="summary_pref_sequence_trigger_timeout">The time allowed to complete a sequence trigger. Default is 1000ms. Can be overridden in a keymap\'s options.</string>
    <string name="title_pref_sequence_trigger_timeout">Default sequence trigger timeout (ms)</string>
    <string name="summary_pref_force_vibrate">Force all remaps to vibrate.</string>
    <string name="title_pref_force_vibrate">Force vibrate</string>
    <string name="title_pref_show_ime_notification">Keyboard picker notification</string>
    <string name="summary_pref_show_ime_notification">Show a persistent notification to allow you to pick a keyboard.</string>
    <string name="title_pref_show_toggle_remappings_notification">Pause/resume remappings notification</string>
    <string name="summary_pref_show_toggle_remappings_notification">Show a persistent notification which starts/pauses your remaps and has a link to start/stop the accessibility service.</string>
    <string name="title_pref_automatic_backup_location">Automatically backup keymaps to a specified location</string>
    <string name="summary_pref_automatic_backup_location_disabled">No location chosen.</string>
    <string name="title_pref_bluetooth_devices">Choose bluetooth devices…</string>
    <string name="title_pref_auto_show_ime_picker">Automatically show keyboard picker</string>
    <string name="summary_pref_auto_show_ime_picker">When a Bluetooth device you have chosen connects or disconnects, the keyboard picker will show automatically. Choose the devices below.</string>
    <string name="title_pref_show_toast_when_action_performed">Show a toast message when you perform an action</string>
    <string name="summary_pref_show_toast_when_action_performed">A little message will popup when you perform an action.</string>
    <string name="title_pref_debug">Logging</string>
    <string name="summary_pref_debug">Enable logging of important events inside the app so it is easier for the developer (or you) to debug any issues you are having with the app. The app will log some potentially sensitive information, such as when and which keys are being pressed so disable this once you have finished logging. The log will never leave your device without your permission and it can\'t be read by another app or you in a text editor. It will disappear if you uninstall the app. A button to see the log will appear in the menu on the home screen. From here you can delete the log and send it to the developer if you need.</string>
    <string name="title_pref_show_toast_when_error_encountered">Show a message when an error is encountered in the app</string>
    <string name="summary_pref_show_toast_when_error_encountered">A message will be shown on the screen prompting you to log it when the app encounters an error it is aware of.</string>
    <string name="title_pref_auto_change_ime_on_connection">Automatically change the keyboard on Bluetooth connection</string>
    <string name="summary_pref_auto_change_ime_on_connection">The Key Mapper keyboard will be automatically selected when a chosen bluetooth device is connected. Your old keyboard will be automatically selected when the device disconnects.
        There may be a 5-10 second delay after disconnecting your device for the keyboard to change.</string>
    <string name="title_pref_root_permission">Key Mapper has root permission</string>
    <string name="summary_pref_root_permission">Enable this if you want to use features/actions which only work on rooted devices. Key Mapper must have root permission from your root-access-management app (e.g Magisk, SuperSU) for these features to work.
        Only turn this on if you know your device is rooted and you have given Key Mapper root permission.</string>
    <string name="title_pref_show_toast_on_action_error">Show a toast message when performing an action fails</string>
    <string name="summary_pref_toast_on_action_error">A little message will popup when an error is encountered while performing an action.</string>
    <string name="title_pref_dark_theme">Dark Theme</string>
    <string name="title_pref_notification_settings">Notification Settings</string>
    <string name="summary_pref_toggle_keyboard_notification">Switch between the Key Mapper keyboard and your default keyboard when you tap the notification.</string>
    <string name="title_pref_toggle_keyboard_notification">Toggle Key Mapper keyboard notification</string>
    <string name="title_pref_toggle_keyboard_on_toggle_keymaps">Automatically change the keyboard when toggling keymaps</string>
    <string name="summary_pref_toggle_keyboard_on_toggle_keymaps">Automatically select the Key Mapper keyboard when you resume your keymaps and select your default keyboard when pausing them.</string>
